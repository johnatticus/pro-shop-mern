{
  "name": "pro-shop-mern",
  "version": "2.0.0",
  "description": "eCommerce website built on MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnatticus/pro-shop-mern.git"
  },
  "author": "johnatticus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnatticus/pro-shop-mern/issues"
  },
  "homepage": "https://github.com/johnatticus/pro-shop-mern#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.2"
  }
}
